<launch>
	<!-- global rosparam -->
	<param name="use_sim_time" value="true"/>

	<!-- param -->
	<param name="weights_path" type="str" value="$(find dnn_attitude_estimation)/weights/weights.pth"/>
	<!-- MAIN -->
	<node pkg="dnn_attitude_estimation" name="regression_prediction" type="regression_prediction.py" output="screen">
		<remap from="/image_raw" to="/decompressed_image"/>
	</node>
</launch>
